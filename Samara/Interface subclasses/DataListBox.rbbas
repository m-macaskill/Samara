#tag ClassClass DataListBoxInherits ListBox	#tag Event		Sub CellAction(row As Integer, column As Integer)		  // If a cell is editable, a CellAction event occurs when the user edits the cell. "Editing" is defined as exiting the cell after clicking in it.		  		  dim label, contents as string		  		  label = self.Heading(column)  // what is the variable name?		  contents = self.cell(row, column)  // what is the text now cobtained in the cell?		  dataWindow(self.window).editDataValue(label, row, contents)		  		  		End Sub	#tag EndEvent	#tag Event		Function CellBackgroundPaint(g As Graphics, row As Integer, column As Integer) As Boolean		  If row mod 2=0 then		    g.foreColor= &cFFFFFF 'RGB(158,238,255)		  else		    g.foreColor= RGB(236,242,255)  //200, 255, 212)		  end if		  		  g.FillRect 0, 0, g.width, g.height		End Function	#tag EndEvent	#tag Event		Function HeaderPressed(column as Integer) As Boolean		  return true  // for the timebeing, prevent the user from sorting the measurements as we could lose track of their order.		End Function	#tag EndEvent	#tag Event		Function KeyDown(Key As String) As Boolean		  select case asc(key)		    		  case 8, 127  // delete		    self.deleteKey		    return true		    		  case else		    return false		    		    		  end select		  		End Function	#tag EndEvent	#tag Method, Flags = &h1		Protected Sub deleteKey()		  // called in response to the delete key in the keyDown event		  		  dim row, upperBound as integer		  		  upperBound = self.listCount		  		  for row = 0 to upperBound		    if self.selected(row) then		      self.removeRow(row)  // remove that row from the display		      dataWindow(self.window).removeDataAtIndex(row)  // remove that row from the underlying data		    end if		  next		  		  		exception err		  gHandleException err, "deleteKey method of DataListBox class."		End Sub	#tag EndMethod	#tag ViewBehavior		#tag ViewProperty			Name="AutoDeactivate"			V