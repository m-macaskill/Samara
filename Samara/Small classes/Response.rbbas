#tag ClassClass Response	#tag Property, Flags = &h0		#tag Note			What should the correct response have been? e.g. "Yes", "left", "left mouse button" etc.		#tag EndNote		actual As string	#tag EndProperty	#tag Property, Flags = &h0		#tag Note			e.g. was the subject's response correct or incorrect? This is optionally passed in via the XML message, not calculated by Samara.		#tag EndNote		judgment As string	#tag EndProperty	#tag Property, Flags = &h0		#tag Note			This has the same value as the ID tag for one of the stimuli in a given set so that the response can be associated with its appropriate stimulus and not one from another trial.		#tag EndNote		stimulus_ID As string = "undefined"	#tag EndProperty	#tag Property, Flags = &h0		#tag Note			What time was the response sent to the iView file?		#tag EndNote		time As double	#tag EndProperty	#tag Property, Flags = &h0		#tag Note			THE MODE OF RESPONSE (an arbitrary string such as 'keyboard', 'verbal')		#tag EndNote		type As string = "Unspecified"	#tag EndProperty	#tag Property, Flags = &h0		#tag Note			What response did the subject give. E.g. "Yes", "left", "left mouse button" etc.		#tag EndNote		value As string = "Nil"	#tag EndProperty	#tag ViewBehavior		#tag ViewProperty			Name="actual"			Group="Behavior"			Type="string"		#tag EndViewProperty		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue="2147483648"			Type="Integer"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="judgment"			Group="Behavior"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InitialValue="0"			Type="Integer"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			Type="String"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="stimulus_ID"			Group="Behavior"			InitialValue="undefined"			Type="string"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			Type="String"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="time"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			InitialValue="0"			Type="Integer"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="type"			Group="Behavior"			InitialValue="Unspecified"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="value"			Group="Behavior"			InitialValue="Unspecified"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty	#tag EndViewBehaviorEnd Class#tag EndClass