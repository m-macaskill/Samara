#tag ClassProtected Class PursuitStimulus	#tag ComputedProperty, Flags = &h0		#tag Getter			Get			  return mAmplX			End Get		#tag EndGetter		#tag Setter			Set			  mAmplX = value			End Set		#tag EndSetter		amplX As double	#tag EndComputedProperty	#tag ComputedProperty, Flags = &h0		#tag Getter			Get			  return mAmplY			End Get		#tag EndGetter		#tag Setter			Set			  mAmplY = value			End Set		#tag EndSetter		amplY As double	#tag EndComputedProperty	#tag Property, Flags = &h0		colour As color	#tag EndProperty	#tag ComputedProperty, Flags = &h0		#tag Getter			Get			  return mDuration			End Get		#tag EndGetter		#tag Setter			Set			  mDuration = value			  self.offsetTime = self.onsetTime + value			End Set		#tag EndSetter		duration As double	#tag EndComputedProperty	#tag Property, Flags = &h1		Protected mAmplX As double = 0.0	#tag EndProperty	#tag Property, Flags = &h1		Protected mAmplY As double = 0.0	#tag EndProperty	#tag Property, Flags = &h1		Protected mDuration As double	#tag EndProperty	#tag Property, Flags = &h1		Protected mOffsetTime As double	#tag EndProperty	#tag Property, Flags = &h1		Protected mPeakVx As double	#tag EndProperty	#tag Property, Flags = &h1		Protected mPeakVy As double	#tag EndProperty	#tag ComputedProperty, Flags = &h0		#tag Getter			Get			  return mOffsetTime			End Get		#tag EndGetter		#tag Setter			Set			  mOffsetTime = value			End Set		#tag EndSetter		offsetTime As double	#tag EndComputedProperty	#tag Property, Flags = &h0		onsetTime As double	#tag EndProperty	#tag ComputedProperty, Flags = &h0		#tag Note			When set, calculate the omega (2 * pi * f) parameter		#tag EndNote		#tag Getter			Get			  return mPeakVx			End Get		#tag EndGetter		#tag Setter			Set			  mPeakVx = value  // peak velocity			  			End Set		#tag EndSetter		peakVx As double	#tag EndComputedProperty	#tag ComputedProperty, Flags = &h0		#tag Note			When set, calculate the omega (2 * pi * f) parameter		#tag EndNote		#tag Getter			Get			  return mPeakVy			End Get		#tag EndGetter		#tag Setter			Set			  mPeakVy = value  // peak velocity			End Set		#tag EndSetter		peakVy As double	#tag EndComputedProperty	#tag Property, Flags = &h0		phaseX As double = 90.0	#tag EndProperty	#tag Property, Flags = &h0		phaseY As double = 0.0	#tag EndProperty	#tag Property, Flags = &h0		type As string = "sine"	#tag EndProperty	#tag ComputedProperty, Flags = &h0		#tag Getter			Get			  // from rules of simple harmonic motion:			  dim f as double // frequency			  			  f = self.peakVx/(-2.0*self.amplX*gPi)			  return 2.0 * gPi * f  // omega			End Get		#tag EndGetter		wX As double	#tag EndComputedProperty	#tag ComputedProperty, Flags = &h0		#tag Getter			Get			  // from rules of simple harmonic motion:			  dim f as double // frequency			  			  f = self.peakVy/(-2.0*self.amply*gPi)			  return 2.0 * gPi * f  // omega			End Get		#tag EndGetter		wY As double	#tag EndComputedProperty	#tag ViewBehavior		#tag ViewProperty			Name="amplX"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="amplY"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="colour"			Group="Behavior"			InitialValue="&h000000"			Type="color"		#tag EndViewProperty		#tag ViewProperty			Name="duration"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue="2147483648"			Type="Integer"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InitialValue="0"			Type="Integer"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			Type="String"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="offsetTime"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="onsetTime"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="peakVx"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="peakVy"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="phaseX"			Group="Behavior"			InitialValue="90.0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="phaseY"			Group="Behavior"			InitialValue="0.0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			Type="String"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			InitialValue="0"			Type="Integer"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="type"			Group="Behavior"			InitialValue="sine"			Type="string"			EditorType="MultiLineEditor"		#tag EndViewProperty		#tag ViewProperty			Name="wX"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty		#tag ViewProperty			Name="wY"			Group="Behavior"			InitialValue="0"			Type="double"		#tag EndViewProperty	#tag EndViewBehaviorEnd Class#tag EndClass