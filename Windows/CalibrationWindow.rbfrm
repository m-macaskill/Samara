terStaticText(1).text = ""		  self.Left = screen(0).width - self.width		End Sub	#tag EndEvent	#tag Method, Flags = &h0		Sub enablePointAtIndex(index as integer, usePoint as boolean)		  // called when the user changes the check box in the list of data points		  // The calibration class will recalculate with the point included or not.		  		  mCurrentPostHocCalibrationInstance.enablePointAtIndex(index, usePoint)		  		  		exception err		  gHandleException err, "enablePointAtIndex method of CalibrationWindow."		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub highlightPointAtIndex(index as integer)		  // called when the user selects a row in the list of data points		  		  mCurrentPostHocCalibrationInstance.highlightPoint(index)		  		  		exception err		  gHandleException err, "highlightPointAtIndex method of CalibrationWindow."		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub plotPoints(enabledPoints() as boolean, data() as dataPoint, stimuli() as stimulus, parameters() as dictionary, highlightedPoints() as boolean)		  dim s, key as string		  dim i as integer		  		  		  for i = 0 to 1 // once each for vertical and horizontal data		    		    // plot the data points:		    self.calGraphCanvas(i).plotPoints(enabledPoints, data, stimuli, parameters,i, highlightedPoints)		    		    // list the values:		    self.calibrationListBox1(i).listPoints(enabledPoints, data, stimuli, i)		    		    // display the parameters from the regression calculation as text:		    s = s + "Factor:   " + format(1.0/parameters(i).value("Slope"), "-###.000") + endOfLine		    s = s + "Offset:   " + format(-1.0*parameters(i).value("Intercept"), "-###.000") + endOfLine		    s = s + "r:          " + format(parameters(i).value("r"), "0.000") + endOfLine		    self.parameterStaticText(i).text = s		    s = ""  // clear it for the next iteration		    		  next		  		  // plot the data points and their 2D deviations from their associated stimuli		  self.calGraphCanvas(2).plotPoints(enabledPoints, data, stimuli, parameters, 2, highlightedPoints)		  // list the 2D values:		  self.calibrationListBox1(2).listPoints(enabledPoints, data, stimuli, 2)		  		exception err		  gHandleException err, "plotPoints method of CalibrationWindow."		  		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub setPostHocCalibrationInstance(calibrator as postHocCalibration)		  mCurrentPostHocCalibrationInstance = calibrator		End Sub	#tag EndMethod	#tag Property, Flags = &h1		#tag Note			Set each time the self.plotPoints method is called so we have a way of commmunicating back to the calibration instance			if points are enabled or disabled in the GUI.		#tag EndNote		Protected mCurrentPostHocCalibrationInstance As PostHocCalibration	#tag EndProperty#tag EndWindowCode#tag Events applyPushButtonH	#tag Event		Sub Action()		  // Apply the calibration equation, to all sets, or just the one from which the calibration data came.		  mCurrentPostHocCalibrationInstance.applyParametersToData(FALSE, self.applyToAllSetsCheckBox.value, "horizontal")		  		End Sub	#tag EndEvent#tag EndEvents#tag Events applyPushButtonV	#tag Event		Sub Action()		  // Apply the calibration equation, to all sets, or just the one from which the calibration data came.		  mCurrentPostHocCalibrationInstance.applyParametersToData(FALSE, self.applyToAllSetsCheckBox.value, "vertical")		  		End Sub	#tag EndEvent#tag EndEvents#tag Events applyPushButton2D	#tag Event		Sub Action()		  // Apply the calibration equation, to all sets, or just the one from which the calibration data came.		  mCurrentPostHocCalibrationInstance.applyParametersToData(FALSE, self.applyToAllSetsCheckBox.value, "2D")		  		End Sub	#tag EndEvent#tag EndEvents#tag Events resetPushButtonH	#tag Event		Sub Action()		  // the data should be reset to its orginal raw imported state.		  mCurrentPostHocCalibrationInstance.applyParametersToData(TRUE, self.ap